{
  "name": "entrip",
  "version": "0.1.0-rc.1",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "postinstall": "pnpm --filter @entrip/api exec prisma generate",
    "dev": "cross-env NODE_OPTIONS=--no-warnings NEXT_TELEMETRY_DISABLED=1 pnpm run build:tokens && turbo run dev",
    "dev:web": "pnpm --filter @entrip/web dev",
    "dev:api": "pnpm --filter @entrip/api dev",
    "dev:api:direct": "cd packages/api && pnpm dev",
    "build": "pnpm run build:tokens && pnpm run build:ts && turbo run bundle",
    "build:tokens": "pnpm --filter @entrip/design-tokens build",
    "build:ts": "tsc -b --pretty",
    "build:clean": "tsc -b --clean",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "pnpm -r exec eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:ci": "pnpm -r exec eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "lint:fix": "pnpm -r exec eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,md}\"",
    "type-check": "tsc -p tsconfig.base.json --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=text-summary --coverageReporters=json-summary --maxWorkers=50% --detectOpenHandles",
    "e2e": "pnpm -r exec playwright test --reporter=list",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "test:e2e": "pnpm run build && pnpm run e2e",
    "analyze": "cross-env ANALYZE=true pnpm --filter @entrip/web build",
    "check-env": "ts-node scripts/check-env.ts",
    "update-docs": "ts-node scripts/update-demo.ts",
    "clean": "rm -rf node_modules **/node_modules **/.next **/dist",
    "openapi:build": "pnpm --filter @entrip/api openapi:build",
    "openapi:lint": "npx @redocly/cli lint packages/shared/openapi/openapi.json",
    "openapi:validate": "pnpm run openapi:build && pnpm run openapi:lint",
    "api:types": "openapi-typescript packages/shared/openapi/openapi.json -o packages/shared/src/generated/api.d.ts",
    "gen:api": "openapi-typescript https://api.dev.entrip.io/swagger.json -o packages/api/types.ts",
    "ci-local": "./scripts/ci-local.sh",
    "precommit": "pnpm run api:types && pnpm -w run build:ts && pnpm run lint && pnpm run test",
    "ci": "pnpm run build && pnpm run lint && pnpm run test",
    "ci:full": "turbo run build lint test --parallel"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@storybook/addon-a11y": "^8.0.0",
    "@storybook/addon-essentials": "^8.0.0",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.0.0",
    "@storybook/react-vite": "^8.0.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/express-rate-limit": "^6.0.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.14.200",
    "@types/node": "^20.19.8",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.16",
    "axios": "^1.6.5",
    "clsx": "^2.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^3.6.0",
    "eslint": "8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^1.3.5",
    "next": "14.1.0",
    "npm-run-all": "^4.1.5",
    "openapi-typescript": "^7.8.0",
    "postcss": "^8.4.31",
    "prettier": "^3.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "recharts": "^2.7.2",
    "start-server-and-test": "^2.0.12",
    "storybook": "^8.0.0",
    "style-dictionary": "^4.0.0",
    "swr": "^2.2.4",
    "tailwindcss": "^3.4.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "turbo": "^1.13.0",
    "typescript": "^5.5.4",
    "vite": "^5.0.0",
    "prisma": "^5.22.0",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "msw": {
    "workerDirectory": [
      "apps\\web\\public"
    ]
  },
  "dependencies": {
    "@tanstack/react-query": "^5.80.7",
    "@tanstack/react-table": "8.21.3",
    "@tanstack/table-core": "8.21.3",
    "express-rate-limit": "^8.0.0",
    "lucide-react": "^0.263.0",
    "react-hook-form": "^7.57.0",
    "tailwind-merge": "^2.2.0",
    "zod": "^3.25.64",
    "zustand": "^4.5.7",
    "@prisma/client": "^5.22.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@prisma/client",
      "prisma",
      "@prisma/engines",
      "@bundled-es-modules/glob",
      "@scarf/scarf"
    ]
  }
}
